[user]
  name = Jeff Jewiss
  email = jeffjewiss@gmail.com
[github]
  user = jeffjewiss
[core]
  pager = cat
  quotepath = false
  whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
[alias]
  br = branch
  ci = commit
  co = checkout
  df = diff
  lg = log -p
  st = status
  g = grep -I
  pb = publish-branch
  pp = !"echo 'Pull'; git pull; echo 'Push'; git push"
  rc = rank-contributors
  sm = show-merges
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
  ui = true
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  frag = magenta bold
  meta = yellow bold
  new = green bold
  old = red bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[apply]
  whitespace = nowarn
[branch]
  autosetupmerge = always
[merge]
  summary = true
  tool = Kaleidoscope
[rebase]
  autosquash = true
[url "git://gist.github.com/"]
  insteadOf = "gist:"
[url "git@heroku.com:"]
  insteadOf = "heroku:"
[difftool "Kaleidoscope"]
  cmd = ksdiff --partial-changeset --relative-path \"$MERGED\" -- \"$LOCAL\" \"$REMOTE\"
[difftool]
  prompt = false
[diff]
  tool = Kaleidoscope
[mergetool "Kaleidoscope"]
  cmd = ksdiff --merge --output \"$MERGED\" --base \"$BASE\" -- \"$LOCAL\" --snapshot \"$REMOTE\" --snapshot
[mergetool]
  prompt = false
[credential]
  helper = osxkeychain
[push]
  default = simple
